{
  "name": "lambda-api",
  "version": "1.0.0",
  "description": "Serverless application to pick some posts from wordpress",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "deploy": "sls deploy",
    "invoke:local-date": "serverless invoke local -f date --path src/functions/date/mock.json",
    "invoke:local-hello": "serverless invoke local -f hello --path src/functions/hello/mock.json",
    "invoke:local-random": "serverless invoke local -f random --path src/functions/random/mock.json",
    "eslint": "eslint --fix src",
    "prettier": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "lint": "npm run typecheck && npm run eslint && npm run prettier",
    "prepare": "cd .. && husky install lambda-api/.config/husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "3.4.0",
    "@middy/http-json-body-parser": "3.4.0",
    "wpapi": "1.2.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.5.0",
    "@types/node": "^14.14.25",
    "@types/wpapi": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.7.1",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "Kazuki USHIJIMA",
  "license": "MIT"
}
